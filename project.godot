; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Habituary"
config/version="alpha"
run/main_scene="res://main_window/main_window.tscn"
config/features=PackedStringArray("4.5", "GL Compatibility")
run/low_processor_mode=true
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Cache="*res://autoloads/cache.gd"
Data="*res://autoloads/data/data.gd"
DayTimer="*res://autoloads/day_timer.gd"
Settings="*res://autoloads/settings.gd"
EventBus="*res://autoloads/event_bus.gd"
Utils="*res://autoloads/utils.gd"
Overlay="*res://autoloads/overlay/overlay.tscn"
Log="*res://addons/log/log.gd"

[debug]

settings/gdscript/always_track_call_stacks=true

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/automatic_versioning/plugin.cfg", "res://addons/log/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="uid://b6uigmasjumt4"

